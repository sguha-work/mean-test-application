  "paths": {
    "/teachers": {
      "get": {
        "tags": [
          "teachers"
        ],
        "summary": "Get all teacher details",
        "description": "Get all teacher details",
        "operationId": "getTeachers",
        "responses": {
          "200": {
            "description": "Fetch teacher successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FetchTeachersResponseModel"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/FetchTeachersResponseModel"
                }
              }
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Teachers not found"
          },
          "405": {
            "description": "Validation exception"
          }
        },
        "security": [
          {
            "petstore_auth": [
              "write:pets",
              "read:pets"
            ]
          }
        ]
      },
      "post": {
        "tags": [
          "teachers"
        ],
        "summary": "Create a new teacher entry in database",
        "description": "Create a new teacher entry in database",
        "operationId": "createTeacher",
        "requestBody": {
          "description": "Create a new teacher in database",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateTeacherRequestModel"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/CreateTeacherRequestModel"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/CreateTeacherRequestModel"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Response of successful teacher creation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateTeacherResponseModel"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/CreateTeacherResponseModel"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input"
          }
        },
        "security": [
          {
            "petstore_auth": [
              "write:pets",
              "read:pets"
            ]
          }
        ]
      }
    },
    "/auth/signup": {
      "post": {
        "tags": [
          "login"
        ],
        "summary": "User signup",
        "description": "User signup",
        "operationId": "signup",
        "requestBody": {
          "description": "signup",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Response of successful teacher creation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input"
          }
        },
        "security": [
          {
            "petstore_auth": [
              "write:pets",
              "read:pets"
            ]
          }
        ]
      }
    }
